{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "declaration": true,
    "declarationMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "./dist",
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["example/**/*", "dist/**/*"]
}
